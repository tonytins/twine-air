{
  "build": {
    "beforeBuildCommand": "npm run build:web",
    "beforeDevCommand": "npm start dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/web"
  },
  "package": {
    "productName": "Khipu",
    "version": "0.1.100"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "all": true
      },
      "path": {
        "all": true
      },
      "shell": {
        "execute": true
      },
      "dialog": {
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tonybark.khipu",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Khipu",
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}